{
  "extends": "../../zlux-app-manager/virtual-desktop/plugin-config/tsconfig.base.json",
  "include": [
    "src/**/*.ts",
    //"../zlux-platform/interface/src/mvd-hosting.d.ts"
  ],
  "compileOnSave": false,
